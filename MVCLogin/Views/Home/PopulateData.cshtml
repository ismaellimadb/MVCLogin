@using System.Data
@{
    ViewBag.Title = "PopulateData";
}



<div>
    @if (Model != null)
    {
        <table border="1" style="width:100%">
            <thread>
                <tr>
                    @foreach(System.Data.DataColumn col in Model.Columns)
                    {
                        <th>@col.Caption</th>
                    }
                    </tr>
                    </thread>
            <tbody>

                @foreach (System.Data.DataRow row in Model.Rows)
                {
                    <tr>
                        @foreach (var cell in row.ItemArray)
                        {
                            if (cell != null)
                            {
                                <td>@cell.ToString()</td>
                            }
                        }
                        
                    </tr>
                }
            </tbody>
        </table>
    }
</div>